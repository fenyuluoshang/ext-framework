{
  "compilerOptions": {
    "noImplicitAny": true,
    "target": "ES5",
    "lib": ["DOM", "ESNext"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "experimentalDecorators": true,
    "module": "ESNext",
    "isolatedModules": false,
    "noEmit": false,
    "baseUrl": "./",
  }
}
